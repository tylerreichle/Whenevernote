{
  "name": "whenevernote",
  "version": "1.1.0",
  "description": "Full Stack Web Application inspired by Evernote. Built with React/Redux and Rails",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/tylerreichle/whenevernote.git"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "start": "webpack --watch",
    "postinstall": "webpack",
    "heroku:db:reset": "heroku pg:reset DATABASE --app whenevernote --confirm whenevernote",
    "heroku:db:migrate": "heroku run rake db:migrate --app whenevernote",
    "heroku:db:seed": "heroku run rake db:seed --app whenevernote"
  },
  "engines": {
    "node": "6.10.1",
    "npm": "3.10.10"
  },
  "keywords": [],
  "author": "Tyler Reichle",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "draft-js": "^0.10.1",
    "draft-js-checkable-list-item": "^2.0.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-mixin": "^2.0.2",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-timer-mixin": "^0.13.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-15": "^1.0.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.11.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/test-setup.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
